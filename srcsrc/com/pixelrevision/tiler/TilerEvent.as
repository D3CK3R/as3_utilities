package com.pixelrevision.tiler {		import flash.events.Event;		public class TilerEvent extends Event{				protected var _info:Object;				/**		 * @constructor		 */			public function TilerEvent(type:String, bubbles:Boolean = false, cancelable:Boolean = false){			super(type, bubbles, cancelable);			_info = new Object();		}				/**		 * Additional info to be passed.		 */		public function get info():Object{			return _info;		}			}}